{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "version": "1.0",
  "version_name": "1.0.0",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "minimum_chrome_version": "111",
  "incognito": "spanning",
  "action": {
    "default_title": "__MSG_actionTitle__"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://gemini.google.com/*",
        "https://*.gemini.google.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }
  ],
  "permissions": []
} 